{
  "name": "mma-gym-backend",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest --detectOpenHandles --forceExit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate": "knex migrate:latest --knexfile src/db/knexfile.js",
    "migrate:rollback": "knex migrate:rollback --knexfile src/db/knexfile.js",
    "migrate:status": "knex migrate:status --knexfile src/db/knexfile.js",
    "seed": "knex seed:run --knexfile src/db/knexfile.js"
  },
  "keywords": [
    "mma",
    "gym",
    "fitness",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "description": "Backend API for MMA Gym Application",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "knex": "^3.1.0",
    "pg": "^8.17.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
    "babel-jest": "^30.2.0",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.2.2"
  }
}
